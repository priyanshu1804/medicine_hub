{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/user/(.*)",
      "dest": "/routes/userRoutes.js"
    },
    {
      "src": "/medicine/(.*)",
      "dest": "/routes/medicineRoutes.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
